{
  "name": "vite_react_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:no-open": "vite --no-open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e",
    "do:cy:open" : "start-server-and-test dev:no-open http://localhost:5173/ cy:open",
    "do:cy:run" : "start-server-and-test dev:no-open http://localhost:5173/ cy:run"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bulma": "^0.9.4",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.21.1",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cypress": "^13.6.1",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "msw": "^2.0.11",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
